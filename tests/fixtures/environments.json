{
  "asuminen": {
    "meta": {
      "repository": "City-of-Helsinki/drupal-helfi-asuminen",
      "azure_devops_link": "https://example.com"
    },
    "environments": {
      "local": {
        "path": {
          "fi": "/fi/asuminen",
          "sv": "/sv/boende",
          "en": "/en/housing"
        },
        "address": {
          "protocol": "https",
          "domain": "helfi-asuminen.docker.so"
        },
        "internal_address": {
          "protocol": "http",
          "domain": "helfi-asuminen.docker.so",
          "port": 8080
        }
      },
      "prod": {
        "path": {
          "fi": "/fi/asuminen",
          "sv": "/sv/boende",
          "en": "/en/housing"
        },
        "address": {
          "protocol": "https",
          "domain": "www.hel.fi"
        },
        "internal_address": {
          "protocol": "https",
          "domain": "nginx-asuminen-prod.apps.platta.hel.fi"
        },
        "meta": {}
      },
      "stage": {
        "path": {
          "fi": "/fi/staging-asuminen",
          "sv": "/sv/staging-boende",
          "en": "/en/staging-housing"
        },
        "address": {
          "protocol": "https",
          "domain": "www.hel.fi"
        },
        "internal_address": {
          "protocol": "https",
          "domain": "nginx-asuminen-staging.apps.platta.hel.fi"
        },
        "meta": {
          "openshift_console_link": "https://example.com"
        }
      },
      "test": {
        "path": {
          "fi": "/fi/test-asuminen",
          "sv": "/sv/test-boende",
          "en": "/en/test-housing"
        },
        "address": {
          "protocol": "https",
          "domain": "www.hel.fi"
        },
        "internal_address": {
          "protocol": "https",
          "domain": "nginx-asuminen-test.apps.arodevtest.hel.fi"
        },
        "meta": {
          "openshift_console_link": "https://example.com"
        }
      }
    }
  }
}
