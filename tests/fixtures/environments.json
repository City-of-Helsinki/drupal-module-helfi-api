{
  "asuminen": {
    "meta": {
      "repository": "City-of-Helsinki/drupal-helfi-asuminen",
      "azure_devops_link": "https://example.com"
    },
    "environments": {
      "local": {
        "path": {
          "fi": "/fi/asuminen",
          "sv": "/sv/boende",
          "en": "/en/housing"
        },
        "protocol": "http",
        "domain": "helfi-asuminen.docker.so:8080"
      },
      "prod": {
        "path": {
          "fi": "/fi/asuminen",
          "sv": "/sv/boende",
          "en": "/en/housing"
        },
        "domain": "www.hel.fi",
        "meta": {}
      },
      "stage": {
        "path": {
          "fi": "/fi/staging-asuminen",
          "sv": "/sv/staging-boende",
          "en": "/en/staging-housing"
        },
        "domain": "www.hel.fi",
        "meta": {
          "openshift_console_link": "https://example.com"
        }
      },
      "test": {
        "path": {
          "fi": "/fi/test-asuminen",
          "sv": "/sv/test-boende",
          "en": "/en/test-housing"
        },
        "domain": "helfi-asuminen-test.docker.so",
        "meta": {
          "openshift_console_link": "https://example.com"
        }
      }
    }
  }
}
